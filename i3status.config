general {
    output_format = "i3bar"
#    separator = ""
	interval = 1
}

order += "ethernet enp0s31f6"
order += "wireless wlp4s0"
order += "tztime local"

ethernet enp0s31f6 {
	format_up = " %ip"
	format_down = ""
}

wireless wlp4s0 {
	format_up = " %ip"
	format_down = ""
} 

tztime local {
	format = "%d.%m.%Y %H:%M:%S"
}

volume master {
	format = "V: %volume"
	device = "default"
	mixer = "Master"
}
